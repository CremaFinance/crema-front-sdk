    "inquirer": "^8.2.2",
    "lodash": "^4.17.21",
    "mocha": "^9.1.3",
    "prettier": "^2.5.1",
    "ts-node": "^10.4.0",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typescript": "^4.3.5",
    "yaml": "next"
  },
  "files": [
    "dist"
  ],
  "lint-staged": {
    "*.{ts,tsx}": "eslint --cache --fix",
    "*.{js,json,jsx,html,css,md}": "prettier --write"
