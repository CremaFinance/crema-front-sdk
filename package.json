    "@solana/web3.js": "1.36.0",
    "axios": "^0.27.2",
    "bn.js": "^5.2.0",
    "bs58": "^5.0.0",
    "decimal.js": "^10.3.1",
    "tiny-invariant": "^1.2.0"
  },
  "devDependencies": {
    "@saberhq/tsconfig": "^1.12.66",
    "@rushstack/eslint-patch": "^1.1.0",
    "@types/bn.js": "^5.1.0",
    "@types/bs58": "^4.0.1",
    "@types/chai": "^4.2.22",
    "@types/inquirer": "^8.2.0",
    "@types/lodash": "^4.14.180",
    "@types/mocha": "^9.1.0",
    "@types/node": "^17.0.23",
    "@types/prettier": "^2.4.4",
    "chai": "^4.3.4",
    "commander": "^9.1.0",
    "console-table-printer": "^2.10.0",
    "eslint": "^8.10.0",
